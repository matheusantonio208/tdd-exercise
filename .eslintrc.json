{
  "env": {
      "browser": true,
      "es2020": true,
      "node": true
  },
  "extends": [
      "airbnb-base",
      "prettier"
  ],
  "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
      "prettier/prettier": ["error",{"endOfLine":"auto"}],
      "class-methods-use-this": "off",
      "no-param-reassign": "off",
      "camelcase": "off",
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}
